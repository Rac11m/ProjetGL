<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GestionDeProduit.ShowProducts">
    <children>
        <TableView fx:id="TableProduit" layoutX="9.0" layoutY="169.0" prefHeight="357.0" prefWidth="1000.0">
            <columns>
                <TableColumn fx:id="idTP" prefWidth="85.0" text="idProduit" />
                <TableColumn fx:id="typeTP" prefWidth="65.0" text="Type" />
                <TableColumn fx:id="refTP" prefWidth="90.0" text="Réference" />
                <TableColumn fx:id="desTP" prefWidth="57.0" text="Designation" />
                <TableColumn fx:id="vnTP" prefWidth="78.0" text="Valeur Nutritionnelle" />
                <TableColumn fx:id="dptTP" prefWidth="75.0" text="Date production" />
                <TableColumn fx:id="dprTP" prefWidth="75.0" text="Date Peremption" />
                <TableColumn fx:id="poidsnTP" prefWidth="75.0" text="Poids net" />
                <TableColumn fx:id="prixTP" prefWidth="75.0" text="Prix vente" />
                <TableColumn fx:id="qntTP" prefWidth="75.0" text="Quantité" />
                <TableColumn fx:id="ingdTP" prefWidth="115.0" text="Ingrédients" />
            </columns>
        </TableView>
        <TextField fx:id="SearchTF" layoutX="310.0" layoutY="124.0" promptText="Chercher Produit" />
        <Button fx:id="SearchBtn" contentDisplay="CENTER" layoutX="496.0" layoutY="124.0" mnemonicParsing="false" onAction="#SearchBtn" prefHeight="24.0" prefWidth="79.0" text="Chercher" />
      <Button fx:id="loadAllBtn" layoutX="310.0" layoutY="43.0" mnemonicParsing="false" onAction="#LoadAll" prefHeight="24.0" prefWidth="265.0" text="Tout afficher" />
        <Button fx:id="retourBtn" layoutX="-2.0" mnemonicParsing="false" onAction="#switchToPreviousScene" text="Retour" />
    </children>
</AnchorPane>
